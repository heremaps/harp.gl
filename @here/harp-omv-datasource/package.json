{
  "name": "@here/harp-omv-datasource",
  "version": "0.3.1",
  "description": "HERE OMV Data source",
  "main": "index.js",
  "typings": "index",
  "worker": {
    "name": "@here/harp-omv-datasource-worker",
    "main": "index-worker.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "test": "cross-env mocha --require source-map-support/register $EXTRA_MOCHA_ARGS ./test/OmvTest.js",
    "gen-decoder": "pbjs -t static-module -w commonjs --no-encode --no-verify --no-create lib/proto/vector_tile.proto -o lib/proto/vector_tile.js && pbts lib/proto/vector_tile.js -o lib/proto/vector_tile.d.ts",
    "prepare": "cross-env tsc --sourceMap false $EXTRA_TSC_ARGS"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com:heremaps/harp.gl.git"
  },
  "author": {
    "name": "HERE Europe B.V.",
    "url": "https://here.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@here/harp-datasource-protocol": "^0.3.1",
    "@here/harp-geoutils": "^0.3.1",
    "@here/harp-lines": "^0.2.1",
    "@here/harp-lrucache": "^0.2.1",
    "@here/harp-mapview": "^0.8.1",
    "@here/harp-mapview-decoder": "^0.3.1",
    "@here/harp-transfer-manager": "^0.2.1",
    "@here/harp-utils": "^0.2.2",
    "earcut": "^2.1.3",
    "long": "^4.0.0",
    "protobufjs": "^6.8.4"
  },
  "devDependencies": {
    "@here/harp-test-utils": "^0.2.1",
    "@types/chai": "^4.1.2",
    "@types/earcut": "^2.1.0",
    "@types/long": "^4.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.0",
    "@types/sinon": "^5.0.1",
    "chai": "^4.0.2",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "source-map-support": "^0.5.2",
    "typescript": "^3.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
