{
    "name": "@here/harp-map-theme",
    "version": "0.24.0",
    "description": "Map themes",
    "main": "index.js",
    "typings": "index",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "cross-env mocha --require source-map-support/register $EXTRA_MOCHA_ARGS ./test/*.js",
        "build": "tsc --build $EXTRA_TSC_ARGS",
        "prepare-icons": "ts-node ./scripts/prepareIcons.ts",
        "prepare": "cross-env tsc --sourceMap false $EXTRA_TSC_ARGS && ts-node scripts/prepack.ts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/heremaps/harp.gl.git",
        "directory": "@here/harp-map-theme"
    },
    "author": {
        "name": "HERE Europe B.V.",
        "url": "https://here.com"
    },
    "license": "Apache-2.0",
    "devDependencies": {
        "@here/harp-atlas-tools": "^0.24.0",
        "@here/harp-datasource-protocol": "^0.24.0",
        "@here/harp-fontcatalog": "^0.1.6",
        "@here/harp-test-utils": "^0.24.0",
        "@types/ajv": "^1.0.0",
        "@types/chai": "^4.2.14",
        "@types/mocha": "^8.2.0",
        "@types/ncp": "^2.0.4",
        "ajv": "^6.12.2",
        "chai": "^4.0.2",
        "copyfiles": "^2.2.0",
        "cross-env": "^7.0.2",
        "github-download": "^0.5.0",
        "mkpath": "^1.0.0",
        "mocha": "^8.2.1",
        "ncp": "^2.0.0",
        "source-map-support": "^0.5.19",
        "tmp": "^0.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
