                        "technique": "text",
                        "attr": {
                            "scale": 0.5
                        }
                    },
                    {
                        "when": "$layer == 'road_label' && $level >= 14",
                        "technique": "text",
                        "attr": {
                            "scale": 0.5
                        }
                    },
                    {
                        "when": "$layer == 'water_label'",
                        "technique": "text",
                        "attr": {
                            "scale": 0.5
                        }
                    },
                    {
                        "when": "$layer == 'waterway_label' && $level > 7",
                        "technique": "text",
                        "attr": {
                            "scale": 0.5
                        }
                    },
                    {
                        "when": "$layer == 'housenum_label' && $level >= 14",
                        "technique": "text",
                        "attr": {
                            "label": "house_num",
                            "scale": 0.4,
                            "minZoomLevel": 16
                        }
                    },
                    {
                        "when": "$layer == 'marine_label' && labelrank == 1",
                        "technique": "text",
                        "attr": {
                            "scale": 0.35
                        }
                    },
                    {
                        "when": "$layer == 'marine_label' && labelrank == 2",
                        "technique": "text",
                        "attr": {
                            "scale": 0.4
                        }
                    },
                    {
                        "when": "$layer == 'marine_label' && labelrank == 3",
                        "technique": "text",
                        "attr": {
                            "scale": 0.45
                        }
                    },
                    {
                        "when": "$layer == 'marine_label' && labelrank == 4",
                        "technique": "text",
                        "attr": {
                            "scale": 0.5
                        }
                    },
                    {
                        "when": "$layer == 'marine_label' && labelrank == 5",
                        "technique": "text",
                        "attr": {
                            "scale": 0.55
                        }
                    },
                    {
                        "when": "$layer == 'marine_label' && labelrank == 6",
                        "technique": "text",
                        "attr": {
                            "scale": 0.6
                        }
                    },
                    {
                        "when": "$layer == 'aeroway'",
                        "technique": "fill",
                        "renderOrder": 2,
                        "attr": {
                            "color": "#b6babd"
                        }
                    }
                ]
            }
        ],
        "landmark": [
            {
                "when": "$layer == 'landmark'",
                "technique": "landmark",
                "renderOrder": 110,
                "attr": {
                    "fadeNear": 0.9,
                    "fadeFar": 1.0
                },
                "styles": [
                    {
                        "when": "$enableLandmarkTextures and $hasTexture",
                        "final": true,
                        "technique": "standard-textured",
                        "attr": {
                            "color": "#f7f9f9",
                            "roughness": 1.5,
                            "metalness": 0.6,
                            "emissive": "#f7f9f9",
                            "emissiveIntensity": 0.5,
                            "transparent": true
                        }
                    },
                    {
                        "technique": "standard",
                        "attr": {
                            "color": "#f7f9f9",
                            "roughness": 1.95,
                            "metalness": 0.85,
                            "emissive": "#f7f9f9",
                            "emissiveIntensity": 0.6,
                            "vertexColors": false
                        }
                    }
                ]
            }
        ]
    }
}
