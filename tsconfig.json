{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es2017",
            "dom"
        ],
        "sourceMap": true,
        "baseUrl": ".",
        "rootDir": ".",
        "outDir": "./dist/node_modules/",
        "jsx": "react",
        "jsxFactory": "React.createElement",
        "strict": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "newLine": "LF",
        "noImplicitReturns": true
    },
    "exclude": [
        "node_modules",
        "@here/*/node_modules",
        "dist"
    ]
}
