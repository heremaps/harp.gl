    },
    "license": "Apache-2.0",
    "devDependencies": {
        "@here/harp-fetch": "^0.3.5",
        "@here/harp-utils": "^0.2.6",
        "@types/chai": "^4.0.4",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.0.8",
        "chai": "^4.1.2",
        "cross-env": "^5.2.0",
        "source-map-support": "^0.5.3",
        "typescript": "^3.5.2"
    },
    "publishConfig": {
