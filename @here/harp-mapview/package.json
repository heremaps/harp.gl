    },
    "license": "Apache-2.0",
    "dependencies": {
        "@here/harp-datasource-protocol": "^0.13.0",
        "@here/harp-fetch": "^0.13.0",
        "@here/harp-geometry": "^0.13.0",
        "@here/harp-geoutils": "^0.13.0",
        "@here/harp-lines": "^0.13.0",
        "@here/harp-lrucache": "^0.13.0",
        "@here/harp-materials": "^0.13.0",
        "@here/harp-text-canvas": "^0.13.0",
        "@here/harp-transfer-manager": "^0.13.0",
        "@here/harp-utils": "^0.13.0",
